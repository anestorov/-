<?xml version="1.0" encoding="utf-8"?>

<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="25%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/numeric_key_height">

    <Row>
        <Key android:codes="40" android:keyLabel="(" android:keyEdgeFlags="left"/>
        <Key android:codes="41" android:keyLabel=")" />
        <Key android:codes="47" android:keyLabel="/" />
        <Key android:codes="45" android:keyIcon="@drawable/dash_numeric" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="-1" android:keyLabel="N" android:keyEdgeFlags="left"/>
        <Key android:codes="44" android:keyLabel="Пауза" />
        <Key android:codes="44" android:keyLabel="," />
        <Key android:codes="32" android:keyIcon="@drawable/space_button_numeric" android:keyEdgeFlags="right" />
    </Row>

    <Row>
        <Key android:codes="42" android:keyLabel="*" android:keyEdgeFlags="left"/>
        <Key android:codes="59" android:keyLabel="Изчакване" />
        <Key android:codes="35" android:keyLabel="#" />
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyEdgeFlags="right" />
    </Row>

    <Row>
        <Key android:codes="-123" android:keyLabel="123" android:keyEdgeFlags="left"/>
        <Key android:codes="43" android:keyLabel="+" />
        <Key android:codes="46" android:keyLabel="." />
        <Key android:codes="-3" android:keyIcon="@drawable/tick_numeric" android:keyEdgeFlags="right"/>
    </Row>



</Keyboard>